page-register {
    .item-ios {
        .item-inner {
            border-bottom: 1px solid rgba(229, 229, 229, 0.7)!important;
        }
    }
    .tip {
        height: 1.67rem;
        width: 100%;
        position: relative;
        padding-left: 4%;
        p {
            font-family: PingFangSC-Regular;
            font-size: 1.17rem;
            color: #999999;
            letter-spacing: 2px;
            padding-left: 6%;
            .protocol {
                font-style: normal;
                color: #5C78C1;
            }
        }
        .radio {
            width: 1.17rem;
            height: 1.17rem;
            position: absolute;
            top: 0.15rem;
        }
    }
    .slef_mock {
        width: 100%;
        height: 100%;
        position: fixed;
        background: rgba(95, 93, 93, 0.3);
        z-index: 14;
    }
    .serve {
        width: 90%;
        height: 21rem;
        border: 1px solid #999999;
        border-radius: 4px;
        padding: 10px;
        position: fixed;
        top: 30%;
        left: 5%;
        z-index: 12;
        background: white;
        &>div {
            width: 100%;
            height: 16rem;
            overflow-y: scroll;
            p {
                font-family: PingFangSC-Regular;
                font-size: 12px;
                color: black;
                letter-spacing: 2px;
                text-indent: 2em;
            }
        }
        button {
            width: 100%;
            height: 3rem;
        }
    }
}