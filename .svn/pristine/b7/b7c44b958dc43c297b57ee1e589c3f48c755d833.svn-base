var key = "" +
    "-----BEGIN RSA PRIVATE KEY-----\n" +
    "\n" +
    "-----END RSA PRIVATE KEY-----\n";

function _rsa(paramvalue) {
    var rsa = new RSAKey();
    rsa.readPrivateKeyFromPEMString(key);
    var hSig = rsa.signString(paramvalue, 'sha1');
    var res = hex2b64(hSig);
    return res;
}
module.exports = _rsa;