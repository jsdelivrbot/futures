page-my {
    .my_top {
        width: 100%;
        height: 8.83rem;
        text-align: center;
        p:nth-child(1) {
            font-family: PingFangSC-Regular;
            font-size: 1.25rem;
            letter-spacing: 2px;
            line-height: 1.25rem;
            padding-top: 2rem;
        }
        p:nth-child(2) {
            font-family: PingFangSC-Medium;
            font-size: 3rem;
            letter-spacing: 2px;
            line-height: 3rem;
            padding-top: 1rem;
        }
    }
    .my_nav {
        width: 100%;
        height: 4.58rem;
        padding: 0.83rem 0;
        position: relative;
        .my_but1,
        .my_but2 {
            width: 44%;
            height: 2.92rem;
            margin-top: 0;
            position: absolute;
        }
        .my_but1 {
            left: 4%;
        }
        .my_but2 {
            right: 4%;
        }
    }
    .my_item {
        margin-top: 0.83rem;
        .label-ios {
            font-size: 1.25rem;
        }
    }
}